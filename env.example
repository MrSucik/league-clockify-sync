# OP.GG Configuration
# Your Riot ID (e.g., for "PlayerName#EUW" use OPGG_GAME_NAME=PlayerName and OPGG_TAG_LINE=EUW)
OPGG_GAME_NAME=YourGameName
OPGG_TAG_LINE=YourTag
# Region: euw, eune, na, kr, jp, br, lan, las, oce, tr, ru, ph, sg, th, tw, vn
OPGG_REGION=euw

# Clockify API Configuration
# Get your API token from: https://clockify.me/user/settings
CLOCKIFY_API_TOKEN=your_clockify_api_token
CLOCKIFY_API_BASE=https://api.clockify.me/api
CLOCKIFY_API_DELAY=50

# Application Configuration
NODE_ENV=development
SYNC_DAYS=14

# Project Configuration
LEAGUE_PROJECT_NAME=League of Legends
